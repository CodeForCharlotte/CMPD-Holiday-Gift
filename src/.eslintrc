{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "flowtype/require-valid-file-annotation": [ "error", "always" ],
    "flowtype/require-return-type": ["error", "always", {
      "annotateUndefined": "never",
      "excludeArrowFunctions": true
    }],
    "react/prop-types": 0,
    "no-console": 0,
    "no-duplicate-imports": 0
  },
  "plugins": [
    "react"
  ]
}
