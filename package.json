{
  "name": "cmpd-explorers-christmas-2017",
  "version": "2017.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p watch-server start-server",
    "start-client": "cd packages/cmpd-fe-nominations && parcel public/index.html",
    "build-client": "cd packages/cmpd-fe-nominations && parcel build public/index.html --detailed-report",
    "build-server": "parcel build server/server.ts --target node",
    "start-server": "nodemon --watch dist dist/server.js",
    "watch-server": "parcel server/server.ts --target node",
    "predebug-server": "npm run build-server",
    "debug-server": "",
    "test-server": "",
    "seed": "rimraf run/db.development.sqlite && ts-node server/seeds",
    "prepack": "npm run build-client && npm run build-server",
    "lint": "eslint -f unix --fix --ext js --ext jsx server src env.js env.example.js",
    "bootstrap": "lerna bootstrap",
    "test": "lerna run test",
    "jest": "jest --config=jest.json",
    "dist:watch": "lerna run --parallel dist:watch",
    "clean": "rimraf packages/**/dist",
    "reset": "rimraf packages/**/node_modules"
  },
  "cacheDirectories": ["node_modules", "packages/cmpd-fe-nomiantions/node_modules"],
  "engines": {
    "node": ">=8.9.x"
  },
  "files": [
    "build",
    "server-notypes"
  ],
  "devDependencies": {
    "@nestjs/testing": "^4.5.5",
    "@types/exceljs": "^0.5.2",
    "@types/express": "^4.0.39",
    "@types/jest": "^22.1.2",
    "@types/node": "^8.0.54",
    "@types/pino": "^4.7.1",
    "@types/sequelize": "^4.0.79",
    "cross-env": "^5.0.5",
    "debug": "~2.6.3",
