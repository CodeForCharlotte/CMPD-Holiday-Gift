    "babel-preset-es2015": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "class-validator": "^0.7.3",
    "compression": "^1.6.2",
    "date-fns": "^1.29.0",
    "eslint": "^3.19.0",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-html": "^2.0.1",
    "exceljs": "^0.5.1",
    "express": "~4.15.2",
    "express-jwt": "^5.3.0",
