    "jasmine": "^2.6.0",
    "lerna": "^2.8.0",
    "nodemon": "^1.14.12",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.6.1",
    "quoted-printable": "^1.0.1",
    "rimraf": "^2.6.2",
    "sqlite3": "^3.1.13",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.4",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@google/maps": "^0.4.3",
    "@nestjs/common": "^4.5.1",
    "@nestjs/core": "^4.5.1",
