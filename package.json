{
  "name": "cmpd-explorers-christmas-2017",
  "version": "2017.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon --watch 'server/**/*.ts' --ignore 'server/**/*.spec.ts' --exec 'ts-node' server/server.ts",
    "start-client": "cd packages/cmpd-fe-nomiantions && cross-env-shell NODE_PATH=./src react-scripts start",
    "build-client": "cd packages/cmpd-fe-nomiantions && cross-env-shell NODE_PATH=./src react-scripts build",
    "build-server": "flow-remove-types --extensions '.js,.mail' --out-dir server-notypes server",
    "start-server": "node server-notypes/apps/serve.js",
    "predebug-server": "npm run build-server",
    "debug-server": "node --inspect server-notypes/apps/serve.js",
    "test-server": "npm run build-server -- --quiet && cd server-notypes && jasmine spec/*.spec.js",
    "nodemon-server": "nodemon -x flow-node server/apps/serve.js -w env.js -w server -w node_modules",
    "seed": "rimraf run/db.development.sqlite && ts-node server/seeds",
    "prepack": "npm run build-client && npm run build-server",
    "lint": "eslint -f unix --fix --ext js --ext jsx server src env.js env.example.js",
    "flow": "flow",
    "bootstrap": "lerna bootstrap",
    "test": "lerna run test",
    "dist:watch": "lerna run --parallel dist:watch",
    "clean": "rimraf packages/**/dist",
    "reset": "rimraf packages/**/node_modules"
  },
  "cacheDirectories": ["node_modules", "packages/cmpd-fe-nomiantions/node_modules"],
  "engines": {
    "node": ">=8.9.x"
  },
  "files": ["build", "server-notypes"],
  "main": "server/apps/serve.js",
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/node": "^8.0.54",
    "@types/sequelize": "^4.0.79",
    "cross-env": "^5.0.5",
    "debug": "~2.6.3",
    "faker": "^4.1.0",
    "fuse-box": "^2.5.0-beta.1",
    "jasmine": "^2.6.0",
    "lerna": "^2.8.0",
    "node": "^8.9.4",
    "nodemon": "^1.14.12",
    "quoted-printable": "^1.0.1",
    "rimraf": "^2.6.2",
    "sqlite3": "^3.1.13",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack-create-file-plugin": "^0.1.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "@google/maps": "^0.4.3",
    "@nestjs/common": "^4.5.1",
    "@nestjs/core": "^4.5.1",
    "@nestjs/swagger": "^1.1.4",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/formidable": "^1.0.29",
    "admin-lte": "^2.3.11",
    "aws-sdk": "^2.133.0",
    "axios": "^0.16.2",
    "babel": "^6.23.0",
    "babel-eslint": "https://registry.npmjs.org/babel-eslint/-/babel-eslint-7.2.3.tgz",
    "babel-plugin-transform-flow-strip-types":
      "https://registry.npmjs.org/babel-plugin-transform-flow-strip-types/-/babel-plugin-transform-flow-strip-types-6.22.0.tgz",
    "babel-polyfill": "https://registry.npmjs.org/babel-polyfill/-/babel-polyfill-6.26.0.tgz",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "class-validator": "^0.7.3",
    "compression": "^1.6.2",
    "date-fns": "^1.29.0",
    "eslint": "https://registry.npmjs.org/eslint/-/eslint-3.19.0.tgz",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-flowtype": "https://registry.npmjs.org/eslint-plugin-flowtype/-/eslint-plugin-flowtype-2.33.0.tgz",
    "eslint-plugin-flowtype-errors": "^3.3.1",
    "eslint-plugin-html": "^2.0.1",
    "exceljs": "^0.5.1",
    "express": "~4.15.2",
    "express-jwt": "^5.3.0",
    "express-validator": "^4.2.1",
    "flow-bin": "^0.54.0",
    "flow-remove-types": "^1.2.1",
    "flow-typed": "^2.1.5",
    "font-awesome": "^4.7.0",
    "formidable": "^1.1.1",
    "fs-extra": "^4.0.2",
    "fuse-box-typechecker": "^2.6.3",
    "fuse.js": "^3.2.0",
    "jsonwebtoken": "^7.4.1",
    "jwt-decode": "^2.2.0",
    "moment": "https://registry.npmjs.org/moment/-/moment-2.18.1.tgz",
    "morgan": "^1.8.1",
    "mustache": "^2.3.0",
    "mysql2": "^1.4.2",
    "neoform": "^0.2.0",
    "neoform-plain-object-helpers": "^0.1.0",
    "neoform-validation": "^0.3.0",
    "nodemailer": "^4.0.1",
    "nodemailer-ses-transport": "^1.5.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pino": "^4.7.2",
    "querystring": "https://registry.npmjs.org/querystring/-/querystring-0.2.0.tgz",
    "ramda": "^0.25.0",
    "react-bootstrap": "^0.31.5",
    "react-bootstrap-table": "^4.2.0",
    "react-inline-confirm": "^2.1.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.14",
    "rxjs": "^5.5.6",
    "sequelize": "^4.23.2",
    "sequelize-encrypted": "^1.0.0",
    "ssacl-attribute-roles": "https://registry.npmjs.org/ssacl-attribute-roles/-/ssacl-attribute-roles-0.0.5.tgz",
    "styled-components": "^2.1.0",
    "typeorm": "^0.1.8",
    "typeorm-encrypted": "^0.5.0"
  },
  "workspaces": ["packages/*"],
  "proxy": {
    "/api": {
      "target": "http://localhost:3001",
      "ws": true
    }
  }
}
